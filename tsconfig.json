{
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "outDir": "./dist",
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "allowJs": true,
        "declaration": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
    },
    "include": ["./src"]
}